@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>
@{
    ViewData["Title"] = "Index";
}
<div class="th-bg-darkblue th-div-full p-2 rounded-2">
<h1>List of Roles</h1>
<a asp-action="Create">Create</a>
    <table class="table table-bordered th-light">
        <thead>
            <tr>
                <td>Id</td>
                <td>Name</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var role in Model)
            {
                <tr>
                    <td>@role.Id</td>
                    <td>@role.Name</td>
                </tr>

            }
        </tbody>
    </table>
</div>